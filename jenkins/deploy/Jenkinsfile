pipeline {
    agent any
    stages {
        stage('deploy') {
            steps {
                bat "ssh -i \"C:\keys\ssh_key\" transpara@192.168.1.114 \"killall node || node 'not running'\""
                bat "ssh -i \"C:\keys\ssh_key\" transpara@192.168.1.114 \"(npm run start&)\""
                bat "ssh -i \"C:\keys\ssh_key\" transpara@192.168.1.114 \"killall ngrok || echo 'not running'\""
                bat "ssh -i \"C:\keys\ssh_key\" transpara@192.168.1.114 \"./ngrok http 80 > /dev/null &\""
                bat "ssh -i \"C:\keys\ssh_key\" transpara@192.168.1.114 \"curl http://localhost:4040/api/tunnels\""
            }
        }
    }
    post {
        success {
            echo "Deploy success"
            echo "${BUILD_NUMBER}"
            
        }
    }
}